{
  "manifest_version": 3,
  "name": "Google Sheets Creator",
  "version": "1.0",
  "description": "Создаёт новый документ в Google Таблицах одним кликом.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": ["tabs"]
}
